<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maggiemoheb.bugs">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Profile"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreatePost"
            android:label="@string/title_activity_create_post" />
        <activity
            android:name=".Settings"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".FollowersProfile"
            android:label="@string/title_activity_followers_profile" />
        <activity
            android:name=".FolloweesProfile"
            android:label="@string/title_activity_followees_profile" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".SignUpActivity"
            android:label="@string/title_activity_sign_up" />
        <activity
            android:name=".NewsFeed"
            android:label="@string/title_activity_news_feed" />
        <activity
            android:name=".followers"
            android:label="@string/title_activity_followers" />
        <activity
            android:name=".ViewNotifications"
            android:label="@string/title_activity_view_notifications"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewPost"
            android:label="@string/title_activity_view_post"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
